#include <stdio.h>
#include <stdlib.h>

int main(void) {

	setbuf(stdout,NULL);
	int numero;
	int acumulador;
	int promedio;
	int i;

	for(i=0;i<5;i++){
	printf("Ingrese numero:\n");
	scanf("%d",&numero);
	acumulador=acumulador+numero;
	}

	promedio=acumulador/5;

	return EXIT_SUCCESS;
}
